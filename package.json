    "lodash": "^4.17.4",
    "style-loader": "^0.20.2"
  },
  "dependencies": {
    "ajv": "^6.1.1",
    "bulma": "^0.6.2",
    "jquery": "^3.3.1",
    "latest-version": "^3.1.0",
    "vue": "^2.5.7",
    "vue-sweetalert": "^0.1.18",
    "vue-sweetalert2": "^1.2.4"
  }
}
